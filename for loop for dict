dic = {'a':[1,2], 'b':[3,4], 'c':[5,6]}
def foo(mydict):
    lst = []
    for key, value in mydict.items():
        lst = lst + value
    return sum(lst)

foo(dict)
#result 21

print(dic.values())
#result dict_values([[1, 2], [3, 4], [5, 6]])

[1,2] + [3,4] + [5,6]
result: [1, 2, 3, 4, 5, 6]

sum ([1, 2, 3, 4, 5, 6])
result: 21
